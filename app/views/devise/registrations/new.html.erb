<h2>Sign up</h2>
<%= javascript_include_tag :defaults, "nested_form" %>

<%= simple_nested_form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
<form>


  <%= f.file_field :avatar %>

  <div><%= f.label :first_name %><br />
  <%= f.text_field :first_name %></div>
  


  <div><%= f.label :last_name %><br />
  <%= f.text_field :last_name %></div>

  <div><%= f.label :address %><br />
  <%= f.text_field :address %></div>


  <div><%= f.label :email %><br />
  <%= f.email_field :email %></div>

  <div><%= f.label :password %><br />
  <%= f.password_field :password %></div>

  <div><%= f.label :password_confirmation %><br />
  <%= f.password_field :password_confirmation %></div>
  <h3>Your Skills</h3>
<%= f.fields_for :my_skills do |task| %>
<%= task.label "Skills" %><br />
<%= task.text_field :name %><br />
<%= task.link_to_remove "Remove this skill" %>
<% end %>
<p><%= f.link_to_add "Add a skill", :my_skills %></p>

  <h3>Desired Skills</h3>

<%= f.fields_for :wanted_skills do |task| %>
<%= task.label "Desired Skills" %><br />
<%= task.text_field :sname %><br />
<%= task.link_to_remove "Remove this skill" %>
<% end %>
<p><%= f.link_to_add "Add a skill", :wanted_skills %></p>
  <div><%= f.submit "Sign up" %></div>
<% end %>
</form>
<%= render "links" %>
